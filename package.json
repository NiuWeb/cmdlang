{
  "name": "@bygdle/cmdlang",
  "version": "0.9.1",
  "description": "simple command-line language with support for embed mathematical expressions",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npx ts-patch install && npm run build",
    "build": "lib-build -e src/index.ts -o lib -t tsconfig.json",
    "dev": "lib-build -e src/index.ts -o lib -t tsconfig.json -w true",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "github.com/NiuWeb/cmdlang"
  },
  "keywords": [
    "cmd",
    "language",
    "cli",
    "programming",
    "declarative",
    "math"
  ],
  "author": "bygdle",
  "license": "MIT",
  "devDependencies": {
    "@bygdle/lib-build": "^0.0.6",
    "@types/jest": "^29.5.3",
    "@types/sprintf-js": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-patch": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "sprintf-js": "^1.1.3",
    "@bygdle/expr-parser": "^0.9.0"
  }
}
